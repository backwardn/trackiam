{
  "Arn": "arn:aws:iam::aws:policy/AmazonEC2RolePolicyForLaunchWizard",
  "CreateDate": "2019-11-13T08:05:53Z",
  "DefaultVersionId": "v3",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4CBGI56NFS",
  "PolicyName": "AmazonEC2RolePolicyForLaunchWizard",
  "PolicyVersion": {
    "CreateDate": "2020-03-31T22:01:02Z",
    "Document": {
      "Statement": [
        {
          "Action": "ec2:CreateTags",
          "Condition": {
            "ForAllValues:StringEquals": {
              "aws:TagKeys": [
                "LaunchWizardApplicationType",
                "LaunchWizardResourceGroupID"
              ]
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "cloudformation:DescribeStackResource",
            "cloudformation:DescribeStackResources",
            "cloudformation:DescribeStacks",
            "cloudformation:SignalResource",
            "ec2:Describe*",
            "ec2:ModifyInstanceAttribute",
            "ec2:RebootInstances",
            "ec2:StartInstances",
            "ec2:StopInstances"
          ],
          "Condition": {
            "ForAllValues:StringEquals": {
              "aws:TagKeys": "LaunchWizardResourceGroupID"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:AttachVolume",
            "ec2:CreateVolume",
            "ec2:DescribeInstances",
            "ec2:DescribeVolumes",
            "ec2:ModifyVolume"
          ],
          "Condition": {
            "ForAllValues:StringLike": {
              "aws:TagKeys": "LaunchWizardResourceGroupID"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:DescribeRouteTables",
            "ec2:ReplaceRoute"
          ],
          "Condition": {
            "ForAllValues:StringEquals": {
              "aws:TagKeys": "LaunchWizardApplicationType"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": "logs:Create*",
          "Effect": "Allow",
          "Resource": "arn:aws:logs:*:*:*"
        },
        {
          "Action": [
            "cloudwatch:GetMetricStatistics",
            "ec2:AssociateAddress",
            "ec2:DescribeAddresses",
            "ec2:DescribeImages",
            "ec2:DescribeRegions"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "dynamodb:BatchGetItem",
            "dynamodb:CreateTable",
            "dynamodb:DeleteTable",
            "dynamodb:DescribeTable",
            "dynamodb:PutItem",
            "dynamodb:Query",
            "dynamodb:Scan",
            "dynamodb:UpdateItem",
            "dynamodb:UpdateTable",
            "s3:GetBucketLocation",
            "s3:GetObject",
            "s3:ListBucket",
            "sqs:ReceiveMessage",
            "sqs:SendMessage"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:dynamodb:*:*:table/LaunchWizard*",
            "arn:aws:s3:::launchwizard*",
            "arn:aws:sqs:*:*:LaunchWizard*"
          ]
        },
        {
          "Action": [
            "ec2:DescribeImages",
            "ec2:DescribeRegions"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "logs:DescribeLogGroups",
            "logs:DescribeLogStreams",
            "logs:PutLogEvents",
            "s3:GetBucketLocation",
            "s3:GetObject",
            "s3:ListBucket",
            "s3:PutObject",
            "s3:PutObjectTagging"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:logs:*:*:*",
            "arn:aws:s3:::aws-sap-data-provider/config.properties",
            "arn:aws:s3:::launchwizard*",
            "arn:aws:s3:::launchwizard*/*"
          ]
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v3"
  },
  "UpdateDate": "2020-03-31T22:01:02Z"
}
