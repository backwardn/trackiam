{
  "Arn": "arn:aws:iam::aws:policy/AmazonLambdaRolePolicyForLaunchWizardSAP",
  "CreateDate": "2020-03-30T20:25:12Z",
  "DefaultVersionId": "v2",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAZKAPJZG4NMJOHL3TN",
  "PolicyName": "AmazonLambdaRolePolicyForLaunchWizardSAP",
  "PolicyVersion": {
    "CreateDate": "2020-06-03T16:20:46Z",
    "Document": {
      "Statement": [
        {
          "Action": [
            "ec2:CreateRoute",
            "ec2:DeleteRoute",
            "ec2:DescribeRouteTables"
          ],
          "Condition": {
            "ForAllValues:StringEquals": {
              "aws:TagKeys": "LaunchWizardApplicationType"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:CreateTags",
            "ec2:DescribeInstanceAttribute",
            "ec2:DescribeInstances",
            "ec2:DescribeTags",
            "ec2:ModifyInstanceAttribute",
            "ssm:GetParameter",
            "ssm:sendCommand"
          ],
          "Condition": {
            "StringLike": {
              "ec2:ResourceTag/aws:cloudformation:stack-id": "arn:aws:cloudformation:*:*:stack/LaunchWizard-*/*"
            }
          },
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:DescribeVpcs",
            "ssm:ListCommands"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "s3:DeleteBucket",
            "s3:DeleteObject",
            "s3:DeleteObjectVersion",
            "s3:GetObject",
            "s3:GetObjectVersion",
            "s3:GetObjectVersionAcl",
            "s3:ListBucket",
            "s3:ListBucketVersions",
            "s3:ListObjectVersions",
            "s3:ListObjects",
            "s3:ListObjectsV2",
            "s3:PutObject",
            "s3:PutObjectTagging"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:s3:::launchwizard*",
            "arn:aws:s3:::launchwizard*/*"
          ]
        },
        {
          "Action": [
            "ssm:GetDocument"
          ],
          "Effect": "Allow",
          "Resource": [
            "arn:aws:ssm:::document/AWS-RunShellScript"
          ]
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v2"
  },
  "UpdateDate": "2020-06-03T16:20:46Z"
}
