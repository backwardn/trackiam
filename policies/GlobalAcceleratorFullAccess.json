{
  "Arn": "arn:aws:iam::aws:policy/GlobalAcceleratorFullAccess",
  "CreateDate": "2018-11-27T02:44:44Z",
  "DefaultVersionId": "v5",
  "IsAttachable": true,
  "Path": "/",
  "PolicyId": "ANPAJ3NSRQKPB42BCNRT6",
  "PolicyName": "GlobalAcceleratorFullAccess",
  "PolicyVersion": {
    "CreateDate": "2020-06-16T16:47:10Z",
    "Document": {
      "Statement": [
        {
          "Action": "iam:CreateServiceLinkedRole",
          "Condition": {
            "StringEquals": {
              "iam:AWSServiceName": "globalaccelerator.amazonaws.com"
            }
          },
          "Effect": "Allow",
          "Resource": "arn:aws:iam::*:role/aws-service-role/globalaccelerator.amazonaws.com/AWSServiceRoleForGlobalAccelerator*"
        },
        {
          "Action": "elasticloadbalancing:DescribeLoadBalancers",
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "ec2:DescribeAddresses",
            "ec2:DescribeInstances",
            "ec2:DescribeInternetGateways",
            "ec2:DescribeRegions"
          ],
          "Effect": "Allow",
          "Resource": "*"
        },
        {
          "Action": [
            "globalaccelerator:*"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "IsDefaultVersion": true,
    "VersionId": "v5"
  },
  "UpdateDate": "2020-06-16T16:47:10Z"
}
